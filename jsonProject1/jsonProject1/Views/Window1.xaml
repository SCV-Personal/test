<Window x:Class="jsonProject1.Views.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:jsonProject1.Views"
        mc:Ignorable="d"
        Title="Window1" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="textBoxInput" HorizontalAlignment="Left" Height="145" Margin="22,10,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="160"/>
        <TextBox x:Name="textBoxOutput" HorizontalAlignment="Left" Height="145" Margin="329,10,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="233"/>
        <Button x:Name="btnDeserialize" Content="Button" HorizontalAlignment="Left" Margin="232,47,0,0" VerticalAlignment="Top" Click="btnDeserialize_Click"/>
        <Button x:Name="btnClear" Content="Button" HorizontalAlignment="Left" Margin="232,104,0,0" VerticalAlignment="Top" Click="btnClear_Click"/>

    </Grid>
</Window>
